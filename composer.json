{
    "name": "digipro-media/omnipay-nmi",
    "type": "library",
    "description": "NMI (Network Merchants Inc.) driver for the Omnipay PHP payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "nmi",
        "durango",
        "network",
        "inc"
    ],
    "homepage": "https://github.com/Richtor/omnipay-nmi",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Hansen",
            "email": "jason.a.hansen@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DigiProMedia/omnipay-tests.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DigiProMedia/recurring-payments.git"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\NMI\\" : "src/" }
    },
    "require": {
        "omnipay/common": "~2.0",
        "php": ">=7.2",
        "richtor/recurring-payments": "dev-master",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "omnipay/tests": "~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
